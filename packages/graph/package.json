{
  "name": "@grants-round/graph",
  "version": "0.0.1",
  "license": "AGPL-3.0-only",
  "keywords": [
    "graph",
    "subgraph"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gitcoinco/grants-round.git"
  },
  "homepage": "https://github.com/gitcoinco/grants-round#readme",
  "scripts": {
    "prepare:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml",
    "prepare:goerli": "mustache config/goerli.json subgraph.template.yaml > subgraph.yaml",
    "prepare:optimism": "mustache config/optimism.json subgraph.template.yaml > subgraph.yaml",
    "prepare:local": "mustache config/localhost.json subgraph.template.yaml > subgraph.yaml",
    "prepare:fantom-testnet": "mustache config/fantom-testnet.json subgraph.template.yaml > subgraph.yaml",
    "prepare:fantom": "mustache config/fantom.json subgraph.template.yaml > subgraph.yaml",
    "prepare:mumbai": "mustache config/mumbai.json subgraph.template.yaml > subgraph.yaml",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ psparacino/quadratic-lenster-dev",
    "deploy-dev": "yarn prepare:mumbai && yarn codegen && yarn build && graph deploy --node https://api.thegraph.com/deploy/ psparacino/quadratic-lenster-dev",
    "deploy-local-node": "yarn create-local && yarn prepare:local && yarn codegen && yarn build && yarn deploy-local", 
    "create-local": "graph create --node http://localhost:8020/ psparacino/quadratic-lenster-dev",
    "remove-local": "graph remove --node http://localhost:8020/ psparacino/quadratic-lenster-dev",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 psparacino/quadratic-lenster-dev",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-ts": "0.31.0",
    "@openzeppelin/subgraphs": "^0.1.8"
  },
  "devDependencies": {
    "matchstick-as": "^0.5.2",
    "mustache": "^4.2.0"
  }
}
